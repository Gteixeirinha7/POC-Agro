<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidateFreightWithSameKeyCity</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(AND(ISCHANGED( ExternalId__c ), ISCHANGED( SourceCity__c )))</errorConditionFormula>
    <errorMessage>Código externo e cidade não podem ser iguais.</errorMessage>
</ValidationRule>
